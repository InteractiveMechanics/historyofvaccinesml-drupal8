					<li>
						<a href="javascript:void(0);"><img src="/themes/historyofvaccines/images/google.svg" alt="Google+" class="google" /></a>
					</li>
				</ul>

                <div class="header-search hidden-sm hidden-xs">
					<form action="/{{language.id}}/search/node" method="get" id="search-block-form" accept-charset="UTF-8" data-drupal-form-fields="edit-keys">
          
                    	<div class="search-area">
                    		<input title="
                                    {% if language.id == 'ar' %}أدخِل الكلمة التي تريد / تريدين بحثها{% endif %}
                                    {% if language.id == 'ur' %}اصطلاحات جو آپ  تلاش کرنا چاہتے ہیں درج کریں{% endif %}
                                    {% if language.id == 'hi' %}उन विषयों को दर्ज करें जिनकी आपको तलाश है{% endif %}
                            " autocomplete="off" data-drupal-selector="edit-keys" type="search" id="edit-keys" name="keys" class="form-search form-control" placeholder="
                                    {% if language.id == 'ar' %}أدخِل الكلمة التي تريد / تريدين بحثها{% endif %}
                                    {% if language.id == 'ur' %}اصطلاحات جو آپ  تلاش کرنا چاہتے ہیں درج کریں{% endif %}
